@article{xenonbeam_500MeV,
title = {Isotopic production cross sections of the residual nuclei in spallation reactions induced by 136Xe projectiles on proton at 500MeV},
journal = {Nuclear Physics A},
volume = {899},
pages = {116-132},
year = {2013},
issn = {0375-9474},
doi = {https://doi.org/10.1016/j.nuclphysa.2012.12.119},
url = {https://www.sciencedirect.com/science/article/pii/S0375947412004423},
author = {L. Giot and J.A. Alcántara-Núñez and J. Benlliure and D. Pérez-Loureiro and L. Audouin and A. Boudard and E. Casarejos and T. Enqvist and J.E. Ducret and B. Fernández-Domínguez and M. {Fernández Ordóñez} and F. Farget and A. Heinz and V. Henzl and D. Henzlova and A. Kelić-Heil and A. Lafriaskh and S. Leray and P. Napolitani and C. Paradela and J. Pereira and M.V. Ricciardi and C. Stéphan and K.-H. Schmidt and C. Schmitt and L. Tassan-Got and C. Villagrasa and C. Volant and O. Yordanov},
keywords = {, measured, deduced, Resulting  for Sn, Sb, Te, I, Xe, Cs, Ba presented also in tabular form, Compared with published data at 1000 MeV/nucleon},
abstract = {Around 270 medium-mass residual nuclei, formed in spallation reactions induced by 136Xe projectiles impinging on a liquid hydrogen target at 500 A MeV, have been unambiguously identified at GSI using the magnetic spectrometer FRS. The individual production cross sections and the longitudinal momentum distributions have been determined with high accuracy. These data represent an important constraint for theoretical models describing spallation reactions.}
}

